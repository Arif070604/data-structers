import java.util.Scanner;
class doublell
{
    Node head;
    Node tail;
    class Node
    {
        int data;
        Node next;
        Node prev;
        Node(int val)
        {
            data=val;
            next=null;
            prev=null;
        }
    }
    doublell()
    {
        head=null;
        tail=null;
    }
    public void insertend(int val){
        Node newnode = new Node(val);

        if (head==null){
            head = newnode;
            tail=newnode;
        }
        else{
            tail.next=newnode;
            newnode.prev=tail;
            tail=newnode;
           }
    }
    public void display()
    {
        Node temp=tail;
                System.out.print("Null");

        while(temp!=null)
        {
             System.out.print(temp.data+"<-->");
            temp = temp.;
        }
                System.out.print("Null");

    }
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        doublell list = new doublell();

        int n = s.nextInt();
        for (int i =0 ;i<n;i++){
            int val = s.nextInt();
            list.insertend(val);
    }
    list.display();
}
}
